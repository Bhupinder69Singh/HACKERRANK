#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;
int main() {
    int n;
    cin>>n;
    vector<int> v;
    int j;
    for(int i=0;i<n;i++){        
        cin>>j;
        v.push_back(j);
    }
    reverse(v.begin(),v.end());
    for(int i:v){
        cout<<i<<" ";
    }
    return 0;
    
}
